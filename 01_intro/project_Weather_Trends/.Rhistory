library(readr)
columbus_temp_data <- read_csv("~/Documents/Personal/training/data_analyst/01_intro/columbus_temp_data.csv")
View(columbus_temp_data)
library(readr)
global_temp_data <- read_csv("~/Documents/Personal/training/data_analyst/01_intro/global_temp_data.csv")
View(global_temp_data)
include(ggplot)
include(ggplot2)
library(ggplot2)
ggplot(columbus_temp_data, aes(x=year)) + ylim(12,16) + geom_line(aes(y=moving_avg_14), color="red", size=1) + geom_point(aes(y=avg_temp), size=.5, color="gray")
ggplot(global_temp_data, aes(x=year)) + ylim(12,16) + geom_line(aes(y=moving_avg_14), color="blue", size=1) + geom_point(aes(y=avg_temp), size=.5, color="gray")
ggplot(global_temp_data, aes(x=year)) + ylim(6,10) + geom_line(aes(y=moving_avg_14), color="blue", size=1) + geom_point(aes(y=avg_temp), size=.5, color="gray")
